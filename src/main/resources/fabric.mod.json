{
  "schemaVersion": 1,
  "id": "simpletrackingcompass",
  "version": "${version}",
  "name": "Simple Tracking Compass",
  "description": "Simple Tracking Compass is a Fabric mod that adds a compass HUD to the top-left corner of your screen.\nIt lets you track other players, which can be especially useful for manhunts or other competitive challenges.",
  "authors": [
    "Chorus_Fruuu1t"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/simpletrackingcompass/textures/icon.png",
  "environment": "*",
  "custom": {
    "modmenu:api": true
  },
  "entrypoints": {
    "client": [
      "me.chorus.simpletrackingcompass.client.SimpleTrackingCompassClient"
    ],
    "main": [
      "me.chorus.simpletrackingcompass.SimpleTrackingCompass"
    ],
    "modmenu": [
      "me.chorus.simpletrackingcompass.compat.ModMenuIntegration"
    ]
  },
  "mixins": [
    "simpletrackingcompass.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}"
  }
}
